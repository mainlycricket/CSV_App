							{{- if $hashStringArr -}} , {{- end -}}
						{{- end -}}
					{{- end -}}
				},
			{{- else -}}
				nil,
			{{- end -}}
		); err != nil {
			message := fmt.Sprintf("error while hashing fields: %v", err)
			log.Print(message)
			w.WriteHeader(http.StatusBadRequest)
			w.Write(getJsonResponse(false, message, nil))
			return
		}
	{{ end }}

{{ end }}
